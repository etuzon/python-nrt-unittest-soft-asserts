[run]
branch = True

source =
    nrt_unittest_soft_asserts/

omit =
    venv/*
    tests/*
    */__init__.py
    */tests_execution.py
    nrt_unittest_soft_asserts/test/tests_suite.py

[report]
exclude_lines =
    if __name__ == .__main__.:

[html]
directory = coverage_report/html

[json]
output = coverage_report/report.json
pretty_print = true

[lcov]
output = coverage_report/lcov.info